<?php

/**
 * @file
 * Install and update hooks for the editor experience.
 */

/**
 * Implements hook_update_N().
 */
function wa_editor_experience_update_10001(): void {
  foreach (\Drupal::entityTypeManager()->getStorage('node')->loadByProperties([
    'type' => 'webform',
  ]) as $node) {
    $node->delete();
  }
}
