{
  "name": "drupal/recommended-project",
  "description": "Project template for Drupal projects with a relocated document root",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "homepage": "https://www.drupal.org/project/drupal",
  "support": {
    "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
    "chat": "https://www.drupal.org/node/314178"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
          "name": "justgiving/justgiving",
          "version": "1.0",
          "type": "drupal-vendor",
          "dist": {
              "url": "https://github.com/JustGiving/JustGiving.Api.Sdk/archive/master.zip",
              "type": "zip"
          }
      }
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "azuyalabs/yasumi": "^2.8",
    "baikho/loqate-php": "^2.0",
    "composer/installers": "^2.0",
    "cweagans/composer-patches": "^1.7",
    "drupal/address": "^2.0",
    "drupal/admin_toolbar": "^3.5",
    "drupal/anonymous_token": "^3.0@beta",
    "drupal/azure_storage": "^2.0",
    "drupal/clientside_validation": "^4.1",
    "drupal/config_ignore": "^3.3",
    "drupal/config_split": "^2.0",
    "drupal/core-composer-scaffold": "^10.5",
    "drupal/core-project-message": "^10.5",
    "drupal/core-recommended": "^10.5",
    "drupal/currency": "^3.4",
    "drupal/datalayer": "^2.1",
    "drupal/devel": "^5.4",
    "drupal/diff": "^2.0@beta",
    "drupal/ds": "^3.30",
    "drupal/encrypt": "^3.2",
    "drupal/entity_print": "^2.18",
    "drupal/entity_usage": "^2.0@beta",
    "drupal/field_group": "^4.0",
    "drupal/focal_point": "^2.1",
    "drupal/gin": "^4.1",
    "drupal/google_tag": "^2.0",
    "drupal/group": "^3.3",
    "drupal/group_content_menu_bundles": "^1.0@alpha",
    "drupal/jquery_ui_button": "^2.1",
    "drupal/key": "^1.20",
    "drupal/key_value_field": "^1.4",
    "drupal/layout_paragraphs": "^2.1",
    "drupal/linkit": "^7.0@alpha",
    "drupal/masquerade": "^2.0",
    "drupal/media_directories": "^2.0",
    "drupal/metatag": "^2.0",
    "drupal/node_co_authors": "^1.2",
    "drupal/paragraphs": "^1.18",
    "drupal/paragraphs_clipboard": "^1.0",
    "drupal/pathauto": "^1.13",
    "drupal/preview_link": "^2.2@alpha",
    "drupal/quick_node_clone": "^1.22",
    "drupal/rabbit_hole": "^2.0@alpha",
    "drupal/real_aes": "^2.6",
    "drupal/recaptcha_v3": "^2.0",
    "drupal/redirect": "^1.10",
    "drupal/scheduler": "^2.2",
    "drupal/scheduler_content_moderation_integration": "^3.0",
    "drupal/schema_metatag": "^3.0",
    "drupal/seckit": "^2.0",
    "drupal/security_review": "^3.0",
    "drupal/sharethis": "^3.1",
    "drupal/social_media_links": "^2.10",
    "drupal/stage_file_proxy": "^3.1",
    "drupal/storybook": "^1.0@beta",
    "drupal/stripe_api": "^3.0",
    "drupal/svg_image": "^3.0",
    "drupal/tb_megamenu": "^1.10",
    "drupal/tmgmt_google": "^1.2",
    "drupal/twig_tweak": "^3.4",
    "drupal/views_bulk_operations": "^4.3",
    "drupal/webform": "^6.3@beta",
    "drupal/webform_rest": "^4.2",
    "drupal/webp": "^1.0@RC",
    "drupal/xmlsitemap": "^2.0",
    "drush/drush": "^12.4",
    "justgiving/justgiving": "^1.0",
    "mikehaertl/phpwkhtmltopdf": "^2.5",
    "npm-asset/jquery-validation": "^1.20",
    "oomphinc/composer-installers-extender": "^2.0",
    "wikimedia/composer-merge-plugin": "^2.1"
  },
  "require-dev": {
    "drupal/core-dev": "^10.5",
    "mglaman/phpstan-drupal": "^1.2",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan-deprecation-rules": "^1.2",
    "spaze/phpstan-disallowed-calls": "^3.4",
    "squizlabs/php_codesniffer": "*"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true,
      "oomphinc/composer-installers-extender": true,
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "tbachert/spi": true,
      "wikimedia/composer-merge-plugin": true
    },
    "sort-packages": true
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "docroot/"
      },
      "file-mapping": {
        "[web-root]/sites/development.services.yml": false
      }
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-module"
      ],
      "docroot/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-theme"
      ],
      "vendor/{$name}": [
        "type:drupal-vendor"
      ]
    },
    "drupal-core-project-message": {
      "include-keys": [
        "homepage",
        "support"
      ],
      "post-create-project-cmd-message": []
    },
    "enable-patching": true,
    "patches": {
        "drupal/group_content_menu": {
            "#3504994: Group member should not be allowed ...": "https://www.drupal.org/files/issues/2025-02-06/group_content_menu-manage-menu-items-permission-allows-menu-creation-3504994.patch"
        },
        "drupal/webform_rest": {
            "3133989 - How to retrieve all submissions for a form?": "https://www.drupal.org/files/issues/2023-10-25/webform_rest-load-submission-data-3133989.patch"
        }
    },
    "merge-plugin": {
      "include": [
          "docroot/modules/contrib/webform/composer.libraries.json"
      ]
    }
  }
}
